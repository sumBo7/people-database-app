# Production Environment Variables
# Copy this to .env.production and fill in your values

# Server Configuration
PORT=5000
NODE_ENV=production

# Google OAuth Configuration (Update with your production client ID)
GOOGLE_CLIENT_ID=your_production_google_client_id_here
GOOGLE_CLIENT_SECRET=your_production_google_client_secret_here

# JWT Secret (Generate a secure random string for production)
JWT_SECRET=your_secure_jwt_secret_here

# Database Configuration
DATABASE_URL=./database/people.db

# Frontend URL (Update with your production domain)
CLIENT_URL=https://your-app-name.railway.app
